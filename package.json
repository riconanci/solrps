{
  "name": "solrps",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:seed": "node prisma/seed.js",
    "db:studio": "prisma studio",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "phase2:enable-ui": "echo 'NEXT_PUBLIC_ENABLE_PHASE2=true' >> .env.local",
    "phase2:enable-wallets": "echo 'NEXT_PUBLIC_USE_BLOCKCHAIN=true' >> .env.local",
    "phase2:disable-wallets": "sed -i 's/NEXT_PUBLIC_USE_BLOCKCHAIN=true/NEXT_PUBLIC_USE_BLOCKCHAIN=false/' .env.local"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  },
  "dependencies": {
    "@coral-xyz/anchor": "^0.31.1",
    "@prisma/client": "^5.17.0",
    "@solana/spl-token": "^0.4.14",
    "@solana/wallet-adapter-base": "^0.9.27",
    "@solana/wallet-adapter-react": "^0.15.39",
    "@solana/wallet-adapter-react-ui": "^0.9.39",
    "@solana/wallet-adapter-wallets": "^0.19.37",
    "@solana/web3.js": "^1.98.4",
    "date-fns": "^3.6.0",
    "next": "^15.5.3",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "tailwind-merge": "^2.5.2",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.23.8",
    "zustand": "^4.5.4"
  },
  "devDependencies": {
    "@types/node": "^20.14.11",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.5",
    "postcss": "^8.4.40",
    "prettier": "^3.3.3",
    "prisma": "^5.17.0",
    "tailwindcss": "^3.4.10",
    "typescript": "^5.5.4",
    "vitest": "^2.0.5"
  },
  "engines": {
    "node": ">=18.17.0"
  },
  "keywords": [
    "solana",
    "blockchain",
    "gaming",
    "rock-paper-scissors",
    "play-to-earn",
    "web3"
  ],
  "description": "Rock Paper Scissors play-to-earn game on Solana blockchain with weekly competitions",
  "repository": {
    "type": "git",
    "url": "https://github.com/riconanci/solrps"
  },
  "license": "MIT"
}